{
    "headers": [{
        "source": "**/*.@(jpg|jpeg|gif|png|webp|woff|woff2|css)",
        "headers": [{
            "key": "Cache-Control",
            "value": "max-age=31536000"
        }]
    }, {
        "source": "**/*",
        "headers": [{
            "key": "Cross-Origin-Opener-Policy",
            "value": "same-origin"
        }, {
            "key": "Cross-Origin-Embedder-Policy",
            "value": "require-corp"
        }, {
            "key": "Strict-Transport-Security",
            "value": "max-age=31536000; includeSubDomains"
        }, {
            "key": "X-FRAME-OPTIONS",
            "value": "require-corp"
        }, {
            "key": "X-XSS-Protection",
            "value": "1; mode=block"
        }]
    }]
}